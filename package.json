{
	"name": "btd6-auto-roles",
	"version": "1.0.0",
	"description": "Discord bot for automatically managing BTD6 achievement-based roles",
	"type": "module",
	"main": "dist/main.js",
	"scripts": {
		"build": "tsc",
		"start": "node dist/main.js",
		"dev": "tsx watch src/main.ts",
		"prisma:generate": "prisma generate",
		"prisma:migrate": "prisma migrate dev",
		"prisma:studio": "prisma studio",
		"lint": "eslint src --ext .ts",
		"type-check": "tsc --noEmit"
	},
	"keywords": [
		"discord",
		"bot",
		"btd6",
		"roles"
	],
	"author": "",
	"license": "MIT",
	"dependencies": {
		"@prisma/client": "^5.19.0",
		"discord.js": "^14.14.1",
		"dotenv": "^16.4.5"
	},
	"devDependencies": {
		"@types/node": "^20.14.0",
		"@typescript-eslint/eslint-plugin": "^7.13.0",
		"@typescript-eslint/parser": "^7.13.0",
		"eslint": "^8.57.0",
		"prisma": "^5.19.0",
		"tsx": "^4.16.0",
		"typescript": "^5.5.0"
	},
	"engines": {
		"node": ">=20.0.0"
	}
}

